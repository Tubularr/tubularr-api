{
  "compilerOptions": {
    "allowJs": true,
    "declaration": false,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": [
      "es7"
    ],
    "module": "commonjs",
    "noImplicitAny": false,
    "noLib": false,
    "noUnusedLocals": false,
    "outDir": "./dist",
    "removeComments": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "ES6"
  }
}
